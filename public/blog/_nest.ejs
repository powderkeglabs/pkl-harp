<header class="article" style="background-image: url(<%= image %>)">
  <div class="container">
    <div class="row">
      <div class="col s12 center-align flex">
      </div>
    </div>
  </div>
</header>

<main>
  <section class="article">
    <div class="container">
      <div class="col s12">
        <article>
          <h3><%= title %></h3>
          <div class="article-meta">
              <time class="time"><%= date %></time>
          </div>
          <%- yield %>
        </article>
      </div>
    </div>
  </section>

  <%- partial("../_shared_partials/learn_more", {}) %>
</main>
